{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "training model",
      "type": "python",
      "request": "launch",
      "cwd": "${workspaceFolder}/keras_training",
      "console": "internalConsole",
      "program": "${workspaceFolder}/keras_training/trainer/task.py",
      "args": [
        "--job-version", "6", "--trainded-dir", "gs://co-oortiz-internal/"
      ],
      "env": {
        "GOOGLE_APPLICATION_CREDENTIALS": "${workspaceFolder}/keras_webapi/webservice-key.json",
      },
    },
    {
      "name": "evaluate model (local)",
      "type": "python",
      "request": "launch",
      "cwd": "${workspaceFolder}/keras_training",
      "console": "internalConsole",
      "program": "${workspaceFolder}/keras_training/trainer/task.py",
      "args": [
        "--job-version", "6", "--is-test", "true", "--img-index", "0"
      ]
    },
    {
      "name": "webapi",
      "type": "python",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "console": "internalConsole",
      "program": "${workspaceFolder}/keras_webapi/main.py",
      "env": {
        "GOOGLE_APPLICATION_CREDENTIALS": "${workspaceFolder}/keras_webapi/service-key.json",
        "GOOGLE_CLOUD_PROJECT": "co-oortiz-internal",
        "MODEL_NAME": "keras_model",
        "PORT": "8080",
      },
    },
  ]
}